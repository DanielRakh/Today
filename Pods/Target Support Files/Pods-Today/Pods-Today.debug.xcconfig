CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AMWaveTransition" "${PODS_ROOT}/Headers/Public/MMScalableVectorView" "${PODS_ROOT}/Headers/Public/POP+MCAnimate" "${PODS_ROOT}/Headers/Public/ReactiveCocoa" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AMWaveTransition" -isystem "${PODS_ROOT}/Headers/Public/MMScalableVectorView" -isystem "${PODS_ROOT}/Headers/Public/POP+MCAnimate" -isystem "${PODS_ROOT}/Headers/Public/ReactiveCocoa" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = -ObjC -l"Pods-Today-AMWaveTransition" -l"Pods-Today-MMScalableVectorView" -l"Pods-Today-POP+MCAnimate" -l"Pods-Today-ReactiveCocoa" -l"Pods-Today-pop" -l"c++" -framework "CoreGraphics" -framework "Foundation" -framework "UIKit"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods